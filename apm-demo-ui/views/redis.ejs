<% layout('layout/_layout') -%>

<h3 style="text-align: center">K/V 缓存</h3>
<br/><br/>
<div>
    <form class="form-horizontal" role="form" action="/api/redis/get" method="get">
        <div class="row">
            <div class="form-group">
                <label for="key" class="col-sm-2 control-label">Key</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="key" value="test-key">
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-default">获取</button>
                </div>
            </div>
        </div>
    </form>
    <br/><hr /> <br/><br/>
    <form class="form-horizontal" role="form" action="/api/redis/set" method="get">
        <div class="row">
            <div class="form-group">
                <label for="key" class="col-sm-2 control-label">Key</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="key" value="test-key">
                </div>
            </div>
            <div class="form-group">
                <label for="value" class="col-sm-2 control-label">Value</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="value" value="test-value">
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-default">设置</button>
                </div>
            </div>
        </div>
    </form>
</div>